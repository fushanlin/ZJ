<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.another.department.dao.DepartmentDao">
    <insert id="regdepartment" parameterType="com.another.department.entity.Department">
        insert into department(name,pid,masterid) VALUES (#{name},#{pid},#{masterid})

    </insert>
</mapper>